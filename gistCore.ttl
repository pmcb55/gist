				a owl:Restriction ;
				owl:onProperty gist:allocatedBy ;
				owl:someValuesFrom [
					a owl:Class ;
					owl:unionOf (
						gist:IntellectualProperty
						gist:Organization
						gist:Person
					) ;
				] ;
			]
			[
				a owl:Restriction ;
				owl:onProperty gist:uniqueText ;
				owl:someValuesFrom xsd:string ;
			]
		) ;
	] ;
	.

gist:InformationQuantity
	a owl:Class ;
	rdfs:label "Information Quantity"^^xsd:string ;
