gist:Language
	a owl:Class ;
	rdfs:label "Language"^^xsd:string ;
	rdfs:comment
		"A recognized, organized set of symbols and grammar."^^xsd:string ,
		"EXAMPLES: Natural languages such as English and Spanish; computer languages such as OWL, Python, and XML."^^xsd:string
		;
	owl:disjointWith
		gist:Magnitude ,
		gist:Organization ,
		gist:PhysicalIdentifiableItem ,
		gist:PhysicalSubstance ,
		gist:TimeInstant ,
		gist:UnitOfMeasure
		;
	.

gist:LivingThing
	a owl:Class ;
	rdfs:label "Living Thing"^^xsd:string ;
	rdfs:comment
		"EXAMPLES: A cat, a mushroom, a tree."^^xsd:string ,
		"NEGATIVE EXAMPLES: fictional life forms such as Unicorns or Mickey Mouse."^^xsd:string ,
		"NOTE: In the open world, you must assume that it might have since died."^^xsd:string ,
