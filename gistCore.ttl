				owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
			]
			[
				a owl:Restriction ;
				owl:onProperty gist:toPlace ;
				owl:onClass gist:GeoPoint ;
				owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
			]
		) ;
