		a owl:Class ;
		owl:intersectionOf (
			gist:Specification
			[
				a owl:Restriction ;
				owl:onProperty gist:basedOn ;
				owl:someValuesFrom gist:TimeZone ;
			]
		) ;
	] ;
	.

gist:Transaction
